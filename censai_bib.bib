doi = {10.1086/520086},
eprint = {0703095},
issn = {0067-0049},
journal = {The Astrophysical Journal Supplement Series},
keywords = {Subject headingg s,cosmology,evolution-galaxies,observations-dark matter-galaxies},
month = {sep},
number = {1},
pages = {196--202},
primaryClass = {astro-ph},
publisher = {IOP Publishing},
title = {{The COSMOS Survey: Hubble Space Telescope Advanced Camera for Surveys Observations and Data Processing}},
//url = {https://iopscience.iop.org/article/10.1086/520086 https://iopscience.iop.org/article/10.1086/520086/meta},
volume = {172},
